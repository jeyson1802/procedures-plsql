SELECT * FROM USR_SICOB.T_SCOB_MULTA ORDER BY AUDFECCREACION DESC;
SELECT * FROM USR_SICOB.T_SCOB_EST_MUL_COB_MOTIVO;
SELECT * FROM USR_SICOB.T_SCOB_AREA_COBRANZA;
SELECT * FROM USR_SICOB.T_SCOB_CLASIFICACION_INDECOPI;
SELECT * FROM USR_SICOB.T_SCOB_SUBMATERIA;
SELECT * FROM USR_SICOB.T_SCOB_MATERIA;
SELECT * FROM USR_SICOB.T_SCOB_SEDE;

SELECT * FROM USR_SICOB.T_SCOB_MULTA_DETAL where id_multa = 138193;-- ORDER BY 2 DESC;
SELECT * FROM USR_SICOB.T_SCOB_MULTA_DETAL WHERE ESTADO = '0' ORDER BY 2 DESC;--137979
SELECT * FROM USR_SICOB.T_SCOB_MULTA_DETAL WHERE ID_MULTA = 138190;
SELECT ID_MULTA,COUNT(*) FROM USR_SICOB.T_SCOB_MULTA_DETAL GROUP BY ID_MULTA HAVING COUNT(*) > 2 ORDER BY ID_MULTA DESC;

SELECT * FROM USR_SICOB.T_SCOB_ORGANO_RESOLUTIVO;

SELECT * FROM USR_SICOB.T_SCOB_EXPEDIENTE;
SELECT * FROM USR_SICOB.T_SCOB_EST_EXP_COB_MOTIVO;
SELECT * FROM USR_SICOB.T_SCOB_EST_EXP_COB;

SELECT * FROM USR_SICOB.T_SCOB_EXPEDIENTE_MULTA where id_multa = 138185;

SELECT * FROM USR_SICOB.T_SCOB_EXPEDIENTE_PERSONA;
SELECT * FROM USR_SICOB.T_SCOB_PERSONA;

SELECT T.COD_INTERNO, T.TXT_DESCRIPCION
  FROM USR_SICOB.T_SCOB_TABLA_DET T
 WHERE T.ID_TABLA = 34

SELECT * FROM USR_SICOB.T_SCOB_EXPED_CRONOGRAMA_DET;

SELECT * FROM USR_SICOB.V_TAB_MULT_BUS_A;

SELECT * FROM USR_SICOB.T_SCOB_TMP_INSTANCIAS_MULTA2 WHERE ID_MULTA  ='138182'

SELECT * FROM USR_SICOB.T_SCOB_TIPO_PROCEDIMIENTO

SELECT T.COD_INTERNO, T.TXT_DESCRIPCION, T.TXT_VALOR1
  FROM USR_SICOB.T_SCOB_TABLA_DET T
 WHERE T.ID_TABLA = 5
   AND T.COD_INTERNO = V_ESTADO_MULTA
   
   
SELECT DISTINCT PERS.*
        FROM USR_SICOB.T_SCOB_MULTA_DETAL MDET
       INNER JOIN USR_SICOB.T_SCOB_MULTA_DENUNCIANTE MDEN
          ON (MDEN.ID_MULTA_DETAL = MDET.ID_MULTA_DETAL AND
             MDEN.ESTADO = '1')
       INNER JOIN USR_SICOB.T_SCOB_PERSONA PERS
          ON (PERS.ID_PERSONA = MDEN.ID_PERSONA)
       INNER JOIN USR_SICOB.T_SCOB_MULTA MULT
          ON (MULT.ID_MULTA = MDET.ID_MULTA)
       WHERE MDET.FLG_ACTUAL = '1'
         AND MDET.ID_MULTA = 138193;
         
SELECT DISTINCT PERS.*
        FROM USR_SICOB.T_SCOB_MULTA_DETAL MDET
       INNER JOIN USR_SICOB.T_SCOB_MULTA_SANCIONADO MSAN
          ON MSAN.ID_MULTA_DETAL = MDET.ID_MULTA_DETAL
         AND MSAN.ESTADO = '1'
         AND NVL(MSAN.COD_TIPO_PERSONA, '1') = '1'
       INNER JOIN USR_SICOB.T_SCOB_PERSONA PERS
          ON PERS.ID_PERSONA = MSAN.ID_PERSONA
       INNER JOIN USR_SICOB.T_SCOB_MULTA MULT
          ON (MULT.ID_MULTA = MDET.ID_MULTA)
       WHERE MDET.FLG_ACTUAL = '1'
         AND MDET.ID_MULTA = 138193;
